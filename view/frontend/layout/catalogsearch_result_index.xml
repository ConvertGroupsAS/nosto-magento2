<?xml version="1.0"?>
<!--suppress XmlPathReference -->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <body>
        <!-- Adds 2 content placeholders; 1 before and 1 after the search result -->
        <referenceContainer name="content">
            <block class="Nosto\Tagging\Block\Search" name="nosto.search"/>
            <block class="Nosto\Tagging\Block\Element" name="nosto.page.search1" before="search.result">
                <action method="setData">
                    <argument name="name" xsi:type="string">nostoId</argument>
                    <argument name="value" xsi:type="string">nosto-page-search1</argument>
                </action>
            </block>
            <block class="Nosto\Tagging\Block\Element" name="nosto.page.search2" after="nosto.page.search1">
                <action method="setData">
                    <argument name="name" xsi:type="string">nostoId</argument>
                    <argument name="value" xsi:type="string">nosto-page-search2</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>